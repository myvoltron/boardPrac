<div>
    <h2>Board</h2>
    <div>
        <a href="/board/new">글 쓰기</a>
    </div>
    <table id="index">
        <thead>
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>date</th>
            </tr>
        </thead>
        <tbody>
            <% result.forEach(elem => { %>
                <tr>
                    <td><%= elem.id%></td>
                    <td><a href="/board/<%= elem.id%>"><%= elem.title%></a></td>
                    <td><%= elem.created_at%></td>
                </tr>
                <% }); %>
        </tbody>
    </table>
</div>